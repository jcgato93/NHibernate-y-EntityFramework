<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="Parcial.Logica.ModelNH.NHProducto, Parcial.Logica" table="tbProducto">
    <id name="Codigo" column="prodCodigo" type="Int32">
      <generator class="native"/>
    </id>
    <property name="Descripcion" column="prodDescripcion" type="String" length="50"/>
    <property name="Cantidad" column="prodCantidad" type="Int32" />
    <property name="Valor" column="prodValor" type="Int32" />
    <property name="IdentificacionProv" column="provIdentificacion" type="Int32" />
    <property name="CodigoCat" column="cateCodigo" type="Int32" />
  </class>
</hibernate-mapping>